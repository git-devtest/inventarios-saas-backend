{
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 14,
    "workbench.colorTheme": "Default Dark Modern",
    "files.watcherExclude": {
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/.angular/**": true
    },
    "search.exclude": {
        "**/node_modules/**": true,
        "**/dist/**": true
    },
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "typescript.disableAutomaticTypeAcquisition": true,
    "git.autofetch": false,
}